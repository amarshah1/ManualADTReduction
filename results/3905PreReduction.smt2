
(set-logic ALL) 

(declare-datatype MyList ((Nil) (Cons (head Real) (tail MyList)) )) 
(declare-fun l0 () MyList) 
(declare-fun l1 () MyList) 
(declare-fun l2 () MyList) 
(declare-fun l3 () MyList) 
(declare-fun i0 () Real) 
(declare-fun i1 () Real) 
(assert (= l1 l1) )
(assert (is-Nil Nil) )
(assert (not (= l1 Nil)) )
(assert (or (is-Cons Nil) (= l2 Nil)) )
(assert (and (is-Cons l2) (is-Cons l2) (is-Cons Nil)) )
(assert (and (is-Nil Nil) (= l0 l2) (is-Nil Nil)) )
(assert (= l1 Nil) )
(assert (and (= l2 Nil) (= l0 Nil) (= l2 Nil) (= l3 Nil)) )
(check-sat)
