
(set-logic ALL) 

(declare-datatype MyList ((Nil) (Cons (head Real) (tail MyList)) )) 
(declare-fun l0 () MyList) 
(declare-fun l1 () MyList) 
(declare-fun l2 () MyList) 
(declare-fun l3 () MyList) 
(declare-fun i0 () Real) 
(declare-fun i1 () Real) 
(assert (and (= l1 Nil) (= l3 l3)) )
(assert (= l1 l0) )
(assert (is-Cons l1) )
(assert (= l3 Nil) )
(assert (= l0 l2) )
(assert (or (= l0 l2) (is-Nil Nil) (is-Cons l1)) )
(assert (not (= l3 l2)) )
(assert (is-Cons l1) )
(check-sat)
