
(set-logic ALL) 

(declare-datatype MyList ((Nil) (Cons (head Real) (tail MyList)) )) 
(declare-fun l0 () MyList) 
(declare-fun l1 () MyList) 
(declare-fun l2 () MyList) 
(declare-fun l3 () MyList) 
(declare-fun i0 () Real) 
(declare-fun i1 () Real) 
(assert (is-Cons l1) )
(assert (is-Cons Nil) )
(assert (not (is-Cons l1)) )
(assert (is-Nil Nil) )
(assert (is-Cons l3) )
(assert (is-Nil l0) )
(assert (or (= l3 Nil) (= l3 l0) (= l2 Nil)) )
(assert (not (is-Cons l2)) )
(check-sat)
