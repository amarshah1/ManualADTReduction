
(set-logic ALL) 

(declare-datatype MyList ((Nil) (Cons (head Real) (tail MyList)) )) 
(declare-fun l0 () MyList) 
(declare-fun l1 () MyList) 
(declare-fun l2 () MyList) 
(declare-fun l3 () MyList) 
(declare-fun i0 () Real) 
(declare-fun i1 () Real) 
(assert (= l0 Nil) )
(assert (is-Cons l1) )
(assert (and (= l0 Nil) (= l2 l0) (= l3 l1) (= l3 Nil)) )
(assert (is-Nil Nil) )
(assert (not (is-Nil l3)) )
(assert (is-Cons Nil) )
(assert (and (= l2 l0) (= l2 Nil) (= l3 Nil) (is-Nil l2)) )
(assert (or (= l1 Nil) (= l0 Nil)) )
(check-sat)
