
(set-logic ALL) 

(declare-datatype MyList ((Nil) (Cons (head Real) (tail MyList)) )) 
(declare-fun l0 () MyList) 
(declare-fun l1 () MyList) 
(declare-fun l2 () MyList) 
(declare-fun l3 () MyList) 
(declare-fun i0 () Real) 
(declare-fun i1 () Real) 
(assert (or (= l1 Nil) (= l3 Nil)) )
(assert (is-Cons l0) )
(assert (= l0 l3) )
(assert (or (is-Cons l1) (= l3 Nil)) )
(assert (and (= l2 l3) (= l1 Nil) (is-Nil Nil) (= l1 l0)) )
(assert (is-Cons Nil) )
(assert (and (= l1 Nil) (= l1 Nil)) )
(assert (not (is-Cons Nil)) )
(check-sat)
