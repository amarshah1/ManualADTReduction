
(set-logic ALL) 

(declare-datatype MyList ((Nil) (Cons (head Real) (tail MyList)) )) 
(declare-fun l0 () MyList) 
(declare-fun l1 () MyList) 
(declare-fun l2 () MyList) 
(declare-fun l3 () MyList) 
(declare-fun i0 () Real) 
(declare-fun i1 () Real) 
(assert (= l0 l2) )
(assert (or (is-Cons Nil) (= l2 Nil)) )
(assert (or (= l0 l3) (= l0 Nil) (is-Nil Nil)) )
(assert (and (= l2 l3) (= l3 Nil)) )
(assert (and (is-Nil l2) (= l3 Nil)) )
(assert (and (= l0 l0) (is-Cons Nil)) )
(assert (or (= l2 l1) (= l3 Nil)) )
(assert (= l2 l1) )
(check-sat)
