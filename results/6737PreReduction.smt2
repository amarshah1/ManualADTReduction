
(set-logic ALL) 

(declare-datatype MyList ((Nil) (Cons (head Real) (tail MyList)) )) 
(declare-fun l0 () MyList) 
(declare-fun l1 () MyList) 
(declare-fun l2 () MyList) 
(declare-fun l3 () MyList) 
(declare-fun i0 () Real) 
(declare-fun i1 () Real) 
(assert (is-Nil Nil) )
(assert (is-Cons Nil) )
(assert (= l1 l1) )
(assert (not (is-Cons Nil)) )
(assert (= l1 Nil) )
(assert (and (is-Cons l3) (is-Nil l3)) )
(assert (= l1 Nil) )
(assert (or (= l1 Nil) (= l3 l2) (= l2 l1)) )
(check-sat)
